# shell

alias l='ls -la'
alias h='history'


# dotfiles

alias rl='source ~/.bashrc'
alias e="${EDITOR} ~/.bashrc"
alias er="e && rl"


# git

alias gs='git status'
alias gb='git branch'
alias gba='git branch'
alias gbd="gb|awk {'print \$2'}|xargs echo git:|sed s/\ // 2> /dev/null"
alias ga="git add $*"
alias gc="git commit . -m $*"
